    "import pandas as pd \n",
    "file_path = input(\"Enter the file path:\")\n",
    "data = pd.read_csv(file_path)\n",
    "mean = data[\"Test Score\"].mean()\n",
    "median = data[\"Test Score\"].median()\n",
    "mode = data[\"Test Score\"].mode()\n",
    "results = pd.DataFrame({\"Mean\":mean,\"Median\":median,\"mode\":mode})\n",
    "print(results)"
